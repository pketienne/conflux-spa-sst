{
	"dependencies": {
		"typescript": "^5.2.2"
	},
	"devDependencies": {
		"@tsconfig/node18": "^18.2.4",
		"@typescript-eslint/eslint-plugin": "^7.7.0",
		"@typescript-eslint/parser": "^7.7.0",
		"aws-cdk-lib": "2.132.1",
		"constructs": "10.3.0",
		"eslint": "^8.57.0",
		"husky": "^8.0.0",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"sst": "^2.41.4",
		"stylelint": "^16.4.0",
		"vitest": "^1.5.0"
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"pre-commit": "lint-staged"
		}
	},
	"name": "conflux-spa-sst",
	"private": true,
	"scripts": {
		"build": "sst build",
		"console": "sst console",
		"deploy": "sst deploy",
		"dev": "sst dev",
		"format": "prettier --write .",
		"lint": "eslint . --ext .ts --ext .tsx",
		"remove": "sst remove",
		"test": "sst bind vitest run",
		"typecheck": "tsc --noEmit"
	},
	"type": "module",
	"version": "0.0.0",
	"workspaces": [
		"packages/*"
	]
}
